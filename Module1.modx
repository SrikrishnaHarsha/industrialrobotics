        MoveL dropapproach,v1000,fine,Servo\WObj:=dropTablew;
        MoveAbsJ JointTarget_1,v1000,z100,Servo\WObj:=pickTablew;
    ENDPROC
    
    PROC Gripper_open()
        waittime 0.5;
        reset do_Gripper_close;
        set do_Gripper_open;
        waittime 0.5;
    ENDPROC
    
    PROC Gripper_close()
