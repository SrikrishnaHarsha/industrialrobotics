        MoveAbsJ JointTarget_1,v1000,z100,Servo\WObj:=pickTablew;
        MoveJ dropapproach,v1000,fine,Servo\WObj:=dropTablew;
        MoveL droppoint,v1000,fine,Servo\WObj:=dropTablew;
